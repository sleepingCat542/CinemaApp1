<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                    xmlns:local="clr-namespace:CinemaApp">
	<SolidColorBrush x:Key="Main" Color="#FF5722"/>
	<SolidColorBrush x:Key="Accent" Color="#0BB7E4"/>
	<SolidColorBrush x:Key="LightMain" Color="#F76A51"/>
	<SolidColorBrush x:Key="LightAccent" Color="#DA71F2"/>
	<SolidColorBrush x:Key="VeryLightText" Color="#FFFBE9E7"/>
	
	
		<LinearGradientBrush x:Key="OrangeGradient" EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" FrameworkElement.FlowDirection="RightToLeft">
			<GradientStop Color="#FFD84315" Offset="1"/>
			<GradientStop Color="#FFFFCCBC"/>
			<GradientStop Color="#FFFF7043" Offset="0.511"/>
		</LinearGradientBrush>
	
	


	<Style x:Key="FontB" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="30"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource LightMain}"></Setter>
	</Style>

	<Style x:Key="FontBW" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="28"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource VeryLightText}"></Setter>
	</Style>

	<Style x:Key="FontM" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="20"></Setter>
	</Style>

	<Style x:Key="FontS" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="14"></Setter>
	</Style>


	<Style x:Key="mini_img" TargetType="materialDesign:PackIcon">
		<Setter Property="Margin" Value="0 5 10 0"></Setter>
		<Setter Property="Height" Value="35"></Setter>
		<Setter Property="Width" Value="35"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource Main}"></Setter>
	</Style>

	<Style x:Key="FontMB" TargetType="TextBox"  BasedOn="{StaticResource MaterialDesignTextBox}">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="12"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource LightMain}"></Setter>
	</Style>

	<Style x:Key="FontMBB" TargetType="TextBox"  BasedOn="{StaticResource MaterialDesignTextBox}">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="16"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource VeryLightText}"></Setter>
	</Style>

	<Style x:Key="FontPMB" TargetType="PasswordBox"  BasedOn="{StaticResource MaterialDesignPasswordBox}">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="12"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource LightMain}"></Setter>
	</Style>

	<Style x:Key="FontML" TargetType="TextBox">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="12"></Setter>
	</Style>

	<Style x:Key="FontMML" TargetType="TextBox">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="12"></Setter>
	</Style>

	<Style x:Key="FontTBl" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="24"></Setter>
		<Setter Property="VerticalAlignment" Value="Center"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource VeryLightText}"></Setter>
	</Style>

	<Style x:Key="InsertCombo" TargetType="ComboBox">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="10"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource LightMain}"></Setter>
	</Style>

	<Style x:Key="CheckEdition" TargetType="CheckBox">
		<Setter Property="FontFamily" Value="Segoe Print Bold"></Setter>
		<Setter Property="FontSize" Value="10"></Setter>
		<Setter Property="Foreground" Value="{DynamicResource LightMain}"></Setter>
		<Setter Property="Background" Value="{x:Null}"></Setter>
	</Style>
